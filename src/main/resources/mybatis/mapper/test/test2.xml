<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ream.www.maventest.mapper.Test2Mapper">
    <insert id="insertOne">
        insert INTO test2  (name) VALUE (#{name})
    </insert>
    <select id="selectTest" resultType="com.ream.www.maventest.po.Test2">
        select * from test2 where id = #{id}
    </select>
    <select id="getAll" resultType="com.ream.www.maventest.po.Test2">
        SELECT * from test2
    </select>
</mapper>